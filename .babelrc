{
  "presets": ["@babel/preset-env"],
  "plugins": [
    ["@babel/transform-runtime"],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "#config": "./src/config",
          "#controllers": "./src/controllers",
          "#constants": "./src/constants",
          "#helper": "./src/helpers",
          "#models": "./src/models",
          "#modules": "./src/modules",
          "#middleware": "./src/middleware",
          "#utils": "./src/utils"
        }
      }
    ]
  ]
}
